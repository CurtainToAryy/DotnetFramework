<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>DotnetFramework.Solr</PackageId>
    <Authors>liubo</Authors>
    <Company>liubo</Company>
    <Product>DotnetFramework</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>通用.net类库Solr 分词检索帮助类库</Description>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Flurl.Http.Signed" Version="2.3.2" />
